@article{loibl2020,
  title = {The Effect of Contrasting Cases during Problem Solving Prior to and after Instruction},
  author = {Loibl, Katharina and Tillema, Marcel and Rummel, Nikol and Van Gog, Tamara},
  year = {2020},
