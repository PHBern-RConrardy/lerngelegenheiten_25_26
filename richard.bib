@article{moseropitz2012,
  title = {{Rechenschwach oder nicht rechenschwach?: Eine kritische Auseinandersetzung mit Diagnosekonzepten, Klassifikationssystemen und Diagnoseinstrumenten unter besonderer Ber{\"u}cksichtigung von {\"a}lteren Sch{\"u}lerinnen und Sch{\"u}lern}},
