@article{chaarani2022,
  title = {Association of {{Video Gaming With Cognitive Performance Among Children}}},
  author = {Chaarani, Bader and Ortigara, Joseph and Yuan, DeKang and Loso, Hannah and Potter, Alexandra and Garavan, Hugh P.},
