@article{girnat2021,
  title = {{Beliefs von Schweizer Sch{\"u}lerinnen und Sch{\"u}lern zum konstruktivistischen und instruktivistischen Lernen im Mathematikunterricht der Sekundarstufe I -- Ergebnisse eines Large-Scale-Assessments zur {\"U}berpr{\"u}fung mathematischer Grundkompetenzen ({\"U}GK) 2016}},
