@article{dejong2023,
  title = {Let's Talk Evidence -- {{The}} Case for Combining Inquiry-Based and Direct Instruction},
  author = {{de Jong}, Ton and Lazonder, Ard W. and Chinn, Clark A. and Fischer, Frank and Gobert, Janice and {Hmelo-Silver}, Cindy E. and Koedinger, Ken R. and Krajcik, Joseph S. and Kyza, Eleni A. and Linn, Marcia C. and Pedaste, Margus and Scheiter, Katharina and Zacharia, Zacharias C.},
  year = {2023},
